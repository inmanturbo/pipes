{
    "name": "inmanturbo/pipes",
    "description": "Pipes for php with a simple api based on functional composition",
    "type": "library",
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "pestphp/pest": "3.x-dev",
        "laravel/pint": "dev-main",
        "phpstan/extension-installer": "1.4.x-dev",
        "phpstan/phpstan-deprecation-rules": "1.2.x-dev",
        "phpstan/phpstan-phpunit": "1.4.x-dev",
        "illuminate/contracts": "^11.20",
        "pestphp/pest-plugin-laravel": "3.x-dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "inmanturbo"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "autoload": {
        "files": [
            "functions.php"
        ]
    }
}
